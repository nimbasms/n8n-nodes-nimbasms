{
  "name": "n8n-nodes-nimbasms",
  "version": "1.0.31",
  "description": "Nimba SMS node for n8n - Send SMS, manage contacts, campaigns and more",
  "license": "MIT",
  "homepage": "https://github.com/nimbasms/n8n-nodes-nimbasms",
  "author": {
    "name": "Nimba SMS",
    "email": "contact@nimbasms.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nimbasms/n8n-nodes-nimbasms.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials index.ts tests --write",
    "lint": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\" \"index.ts\" \"tests/**/*.ts\"",
    "lintfix": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\" \"index.ts\" \"tests/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false",
    "prepublishOnly": "npm run build && npm run test:ci"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/NimbaSmsApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/NimbaSMS/NimbaSMS.node.js"
    ]
  },
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "sms",
    "marketing",
    "nimbasms",
    "nimba",
    "communication",
    "campaigns",
    "sms marketing",
    "sms marketing automation",
    "sms marketing software",
    "sms marketing platform",
    "sms marketing api",
    "sms marketing integration",
    "sms marketing tool",
    "africa",
    "guinea"
  ],
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^18.19.123",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "jest": "^30.1.3",
    "n8n-workflow": "*",
    "prettier": "^2.8.8",
    "ts-jest": "^29.4.1",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
